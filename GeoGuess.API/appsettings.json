{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "GeoGuessDB_SQL_MS": "Server=<YOUR_SQL_SERVER_NAME>;Database=GeoGuessDB;Trusted_Connection=True;TrustServerCertificate=Yes"
  },
